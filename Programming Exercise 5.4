
 
 public class answerConvert {
 public static void main(String[] args) {

	 int count = 0;
	 int miles = 1;
	 double km = 1.609;
	 System.out.println("Miles" + " Kilometers");
	 
    while (count < 10) {
    	System.out.println(miles + " " + km); 
    	
    	miles = miles + 1;
    	km = miles * 1.609;
        count++;
    } 




    
   }
}
